{
  "projectName": "alexai",
  "version": "3.1",
  "frontend": "javascript",
  "javascript": {
    "framework": "react",
    "config": {
      "SourceDir": ".",
      "DistributionDir": ".next",
      "BuildCommand": "npm run build",
      "StartCommand": "npm run start"
    }
  },
  "providers": [
    "awscloudformation"
  ],
  "hosting": {
    "type": "ssr"
  },
  "features": {
    "auth": {
      "enablecaseinsensitivity": true,
      "useinclusiveterminology": true,
      "breakcirculardependency": true,
      "useenabledmfas": true
    },
    "storage": {
      "enableAutoIndexing": true
    },
    "project": {
      "overrides": true
    }
  }
}
