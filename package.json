{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:smart": "./dev-with-build.sh",
    "prebuild": "echo 'Running consolidated build script...'",
    "build": "./scripts/consolidated-build.sh",
    "build:legacy": "next build",
    "build:local": "./local-build.sh",
    "build:standalone": "node scripts/create-standalone.js",
    "postbuild": "echo 'Build completed. Run npm start to start the application.'",
    "start": "./scripts/consolidated-start.sh",
    "start:legacy": "node .next/standalone/server.js",
    "start:next": "next start",
    "start:amplify": "./amplify-start.sh",
    "start:standalone": "cd .next/standalone && node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "amplify:build": "./build.sh",
    "amplify:serve": "node server.js",
    "env:pull": "node scripts/amplify-env-manager.js pull",
    "env:push": "node scripts/amplify-env-manager.js push",
    "env:list": "node scripts/amplify-env-manager.js list",
    "env:openai": "node scripts/amplify-openai-setup.js",
    "test:prod": "chmod +x scripts/production-test.sh && ./scripts/production-test.sh",
    "amplify:local": "./amplify-local-test.sh",
    "test:download": "chmod +x scripts/run-download-tests.sh && ./scripts/run-download-tests.sh",
    "test:download:prepare": "node scripts/prepare-download-test.js",
    "test:pre-commit": "chmod +x scripts/pre-commit-test.sh && ./scripts/pre-commit-test.sh",
    "setup:git-hooks": "node scripts/setup-git-hooks.js",
    "fix:static": "chmod +x scripts/fix-static-assets.sh && ./scripts/fix-static-assets.sh",
    "fix:standalone": "node scripts/create-standalone.js",
    "rebuild": "chmod +x scripts/rebuild-app.sh && ./scripts/rebuild-app.sh",
    "log:build": "node scripts/enhanced-build-logger.js",
    "log:start": "node scripts/enhanced-start-logger.js",
    "log:analyze": "node scripts/enhanced-build-logger.js && node scripts/enhanced-start-logger.js"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "docx": "^9.4.1",
    "html-to-pdf": "^0.1.11",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "next": "15.3.1",
    "openai": "^4.95.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^9.2.1",
    "react-syntax-highlighter": "^15.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-pdf": "^6.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "add": "^2.0.6",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "serve": "^14.2.4",
    "tailwindcss": "^4.1.4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "overrides": {
    "pdfjs-dist": "^3.11.174"
  },
  "resolutions": {
    "pdfjs-dist": "^3.11.174"
  }
}
